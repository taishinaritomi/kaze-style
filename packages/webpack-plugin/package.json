{
  "name": "@kaze-style/webpack-plugin",
  "version": "0.9.0-0",
  "license": "MIT",
  "author": "Taishi Naritomi",
  "description": "Kaze [é¢¨] zero-runtime CSS in JS",
  "repository": {
    "type": "git",
    "url": "https://github.com/taishinaritomi/kaze-style.git",
    "directory": "packages/webpack-plugin"
  },
  "type": "module",
  "sideEffects": false,
  "main": "dist/cjs/index.js",
  "module": "dist/index.js",
  "typings": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./virtualLoader": {
      "import": "./dist/virtualLoader.js",
      "require": "./dist/cjs/virtualLoader.js"
    },
    "./preLoader": {
      "import": "./dist/preLoader.js",
      "require": "./dist/cjs/preLoader.js"
    },
    "./loader": {
      "import": "./dist/loader.js",
      "require": "./dist/cjs/loader.js"
    },
    "./assets/*": "./assets/*"
  },
  "files": [
    "dist",
    "assets",
    "LICENSE"
  ],
  "scripts": {
    "type-check": "tsc --noEmit",
    "dev": "tsx ./../../scripts/build.ts --watch",
    "build": "tsx ./../../scripts/build.ts"
  },
  "dependencies": {
    "@kaze-style/build-man": "^0.9.0-0"
  },
  "peerDependencies": {
    "@kaze-style/react": "^0.9.0-0",
    "webpack": "^5"
  }
}
